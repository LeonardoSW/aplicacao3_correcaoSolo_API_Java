package com.example.demo.calculos;

public class CalculaScmol {

	public CalculaScmol(String potassio, String calcio, String magnesio) {
		
		double qtdpotassio = Double.parseDouble(potassio); 
		double qtdcalcio = Double.parseDouble(calcio);
		double qtdmagnesio = Double.parseDouble(magnesio);
		
	}
	
}
